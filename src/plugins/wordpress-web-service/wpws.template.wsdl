	</wsdl:message>
	<wsdl:message name="getGalleryResponse">
		<wsdl:part name="return" type="typens:Gallery"/>
	</wsdl:message>
	<wsdl:message name="getImagesRequest">
		<wsdl:part name="galleryId" type="xsd:integer"/>
	</wsdl:message>
	<wsdl:message name="getImagesResponse">
		<wsdl:part name="return" type="typens:ArrayOfImage"/>
	</wsdl:message>
	<wsdl:portType name="WordPressHttpPort">
		<wsdl:operation name="getPosts">
			<wsdl:input message="typens:getPostsRequest"/>
			<wsdl:output message="typens:getPostsResponse"/>
		</wsdl:operation>
		<wsdl:operation name="getPost">
