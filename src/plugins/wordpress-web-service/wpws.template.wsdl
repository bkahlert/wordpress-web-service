		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="WordPressHttpBinding" type="typens:WordPressHttpPort">
		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="getPosts">
			<soap:operation soapAction="urn:#getPosts"/>
			<wsdl:input>
				<soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="urn:WordPress"/>
			</wsdl:input>
			<wsdl:output>
