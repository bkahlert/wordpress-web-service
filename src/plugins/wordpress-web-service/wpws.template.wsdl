<?xml version="1.0"?>
<wsdl:definitions xmlns:typens="urn:WordPress" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" name="WordPress" targetNamespace="urn:WordPress">
	<wsdl:types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:WordPress">
			<complexType name="Post">
				<all>
					<element name="id" type="xsd:unsignedInt"/>
					<element name="author" type="xsd:string"/>
					<element name="date" type="xsd:date"/>
					<element name="dateGmt" type="xsd:date"/>
					<element name="content" type="xsd:string"/>
					<element name="title" type="xsd:string"/>
					<element name="excerpt" type="xsd:string"/>
					<element name="status" type="xsd:string"/>
					<element name="commentStatus" type="xsd:string"/>
					<element name="pingStatus" type="xsd:string"/>
					<element name="password" type="xsd:string"/>
					<element name="name" type="xsd:string"/>
					<element name="toPing" type="xsd:string"/>
					<element name="pinged" type="xsd:string"/>
					<element name="modified" type="xsd:date"/>
					<element name="modifiedGmt" type="xsd:date"/>
					<element name="contentFiltered" type="xsd:string"/>
					<element name="parentId" type="xsd:unsignedInt"/>
					<element name="guid" type="xsd:string"/>
					<element name="menuOrder" type="xsd:unsignedInt"/>
					<element name="type" type="xsd:string"/>
					<element name="mimeType" type="xsd:string"/>
					<element name="commentCount" type="xsd:string"/>
					<element name="filter" type="xsd:string"/>
				</all>
			</complexType>
			<complexType name="ArrayOfPost">
				<complexContent>
					<restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType" wsdl:arrayType="typens:Post[]"/>
					</restriction>
				</complexContent>
			</complexType>
			<complexType name="Page">
				<complexContent>
					<extension base="typens:Post"/>
				</complexContent>
			</complexType>
			<complexType name="ArrayOfPage">
				<complexContent>
					<restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType" wsdl:arrayType="typens:Page[]"/>
					</restriction>
				</complexContent>
			</complexType>
			<complexType name="Gallery">
				<all>
					<element name="id" type="xsd:unsignedInt"/>
					<element name="parentId" type="xsd:unsignedInt"/>
					<element name="title" type="xsd:string"/>
					<element name="description" type="xsd:string"/>
					<element name="mainImage" type="typens:Image"/>
					<element name="images" type="typens:ArrayOfImage"/>
					<element name="subGalleries" type="typens:ArrayOfGallery"/>
				</all>
			</complexType>
			<complexType name="ArrayOfGallery">
				<complexContent>
					<restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType" wsdl:arrayType="typens:Gallery[]"/>
					</restriction>
				</complexContent>
			</complexType>
			<complexType name="Image">
				<all>
					<element name="url" type="xsd:string"/>
					<element name="width" type="xsd:unsignedInt"/>
					<element name="height" type="xsd:unsignedInt"/>
					<element name="resizeableUrl" type="xsd:string"/>
					<element name="maxResizeableWidth" type="xsd:unsignedInt"/>
					<element name="maxResizeableHeight" type="xsd:unsignedInt"/>
					<element name="title" type="xsd:string"/>
					<element name="description" type="xsd:string"/>
				</all>
			</complexType>
			<complexType name="ArrayOfImage">
				<complexContent>
					<restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType" wsdl:arrayType="typens:Image[]"/>
					</restriction>
				</complexContent>
			</complexType>
		</schema>
	</wsdl:types>
	<wsdl:message name="getPostsRequest">
		<wsdl:part name="args" type="xsd:string"/>
	</wsdl:message>
	<wsdl:message name="getPostsResponse">
		<wsdl:part name="return" type="typens:ArrayOfPost"/>
	</wsdl:message>
	<wsdl:message name="getPostRequest">
		<wsdl:part name="postId" type="xsd:integer"/>
	</wsdl:message>
	<wsdl:message name="getPostResponse">
